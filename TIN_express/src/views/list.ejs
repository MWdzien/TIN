<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Orders</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>
<body>
<h1>Orders List</h1>


<table>
    <thead>
        <tr>
            <th>Customer</th>
            <th>Total Price</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% orders.forEach(order => { %>
            <tr>
                <td><%= order.customer %></td>
                <td><%= order.total %></td>
                <td><a href="/orders/<%= order.id %>">Details</a> | <a href="/orders/delete/<%= order.id %>">Delete</a></td>
            </tr>
        <% }); %>
    </tbody>
</table>
<a href="/orders/new">Add new order</a>
</body>
</html>